<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:c="http://www.springframework.org/schema/c"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="myOrganization" class="com.tpuhalo.domain.Organization"
		c:name="Amazon" c:year="1999" c:address-ref="address" />

	<bean id="address" class="com.tpuhalo.domain.Address"
		c:street="Blue Berry" c:city="LA" c:zipCode="10000" />
</beans>
